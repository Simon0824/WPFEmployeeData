<Window x:Class="WpfApp16.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp16"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="LightSteelBlue">
        <GroupBox Header="Dane pracownika" HorizontalAlignment="Left" Margin="54,46,0,142" Width="310">
        </GroupBox>
        <Label Content="Imię" HorizontalAlignment="Left" Margin="83,83,0,0" VerticalAlignment="Top"/>
        <TextBox Name="imie" HorizontalAlignment="Left" Margin="197,83,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="153"/>
        <Label Content="Nazwisko" HorizontalAlignment="Left" Margin="83,125,0,0" VerticalAlignment="Top"/>
        <TextBox Name="nazwisko" HorizontalAlignment="Left" Margin="197,133,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="153"/>
        <Label Content="Stanowisko" HorizontalAlignment="Left" Margin="83,178,0,0" VerticalAlignment="Top"/>
        <ComboBox Name="stanowisko" HorizontalAlignment="Left" Margin="197,182,0,0" VerticalAlignment="Top" Width="153">
            <ComboBoxItem Content="Kierownik"/>
            <ComboBoxItem Content="Starszy programista"/>
            <ComboBoxItem Content="Młodszy programista"/>
            <ComboBoxItem Content="Tester"/>
        </ComboBox>
        <Button Background="SteelBlue" BorderBrush="White" Foreground="White" Content="Zatwierdź" HorizontalAlignment="Center"  VerticalAlignment="Top" Width="200" Height="30" Margin="0,351,0,0" Click="Button_Click"/>
        <GroupBox Header="Generowanie hasła" HorizontalAlignment="Right" Margin="0,46,54,142" Width="310"/>
        <TextBox Name="iloscznakow" HorizontalAlignment="Left" Margin="567,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="153"/>
        <Label Content="Ile znaków?" HorizontalAlignment="Left" Margin="483,83,0,0" VerticalAlignment="Top"/>
        <CheckBox Name="maleiduze" Content="Małe i wielkie litery" HorizontalAlignment="Left" Margin="483,138,0,0" VerticalAlignment="Top"/>
        <CheckBox Name="cyfry" Content="Cyfry" HorizontalAlignment="Left" Margin="483,166,0,0" VerticalAlignment="Top"/>
        <CheckBox Name="specjalne" Content="Znaki specjalne" HorizontalAlignment="Left" Margin="483,193,0,0" VerticalAlignment="Top"/>
        <Button BorderBrush="White" Background="SteelBlue" Foreground="White"   Name="generuj" Content="Generuj hasło"  Width="80" HorizontalAlignment="Left" Margin="551,249,0,0" VerticalAlignment="Top" Click="generuj_Click"/>
    </Grid>
</Window>
